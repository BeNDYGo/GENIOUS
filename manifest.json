{
  "manifest_version": 3,
  "name": "Music Extension",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*", "https://genius.com/*", "https://music.yandex.com/*"],
      "js": ["main.js"]
    }
  ]
}