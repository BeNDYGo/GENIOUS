{
  "manifest_version": 3,
  "name": "GeniusButton",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://genius.com/*"],
      "js": ["js/main.js", "js/genius.js"]
    },
    {
      "matches": ["https://soundcloud.com/*"],
      "js": ["js/main.js", "js/soundcloud.js"]
    },
    {
      "matches": ["https://music.yandex.ru/*", "https://music.yandex.com/"],
      "js": ["js/main.js", "js/yandex_music.js"]
    }
  ]
}
